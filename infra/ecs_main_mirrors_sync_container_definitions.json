[
  {
    "name": "${container_name}",
    "image": "${container_image}",
    "memoryReservation": ${container_memory},
    "cpu": ${container_cpu},
    "essential": true,
    "portMappings": [],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "${log_region}",
        "awslogs-stream-prefix": "${container_name}"
      }
    },
    "environment": [{
        "name": "MIRRORS_BUCKET_REGION",
        "value": "${mirrors_bucket_region}"
    },
    {
        "name": "MIRRORS_BUCKET_HOST",
        "value": "${mirrors_bucket_host}"
    },
    {
        "name": "MIRRORS_BUCKET_NAME",
        "value": "${mirrors_bucket_name}"
    },
    {
        "name": "MIRROR_ANACONDA_R",
        "value": "${mirror_anaconda_r}"
    },
    {
        "name": "MIRROR_ANACONDA_CONDA_FORGE",
        "value": "${mirror_anaconda_conda_forge}"
    },
    {
        "name": "MIRROR_ANACONDA_CONDA_ANACONDA",
        "value": "${mirror_anaconda_conda_anaconda}"
    },
    {
        "name": "MIRROR_CRAN",
        "value": "${mirror_cran}"
    },
    {
        "name": "MIRROR_PYPI",
        "value": "${mirror_pypi}"
    },
    {
        "name": "MIRROR_DEBIAN",
        "value": "${mirror_debian}"
    },
    {
        "name": "MIRROR_NLTK",
        "value": "${mirror_nltk}"
    },
    {
        "name": "MIRROR_OPENSTREETMAP",
        "value": "${mirror_openstreetmap}"
    }]
  }
]
